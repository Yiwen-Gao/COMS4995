{
	"name": "yiwen-s-first-extension",
	"displayName": "Yiwen's First Extension",
	"description": "Just playing around with the extension environment :)",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.addData",
				"title": "Add Data"
			},
			{
				"command": "extension.removeData",
				"title": "Remove Data"
			}
		],
		"keybindings": [
			{
				"command": "extension.addData", 
				"key": "Ctrl+Shift+A",
				"mac": "Cmd+Shift+A"
			},
			{
				"command": "extension.removeData",
				"key": "Ctrl+Shift+B",
				"mac": "Cmd+Shift+B"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
